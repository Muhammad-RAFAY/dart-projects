void main() {
  int value1 = 3343;
  print(value1);
  int num1 = value1 % 10;
  print(num1);
  value1 = value1 ~/ 10;
  print(value1);
  int num2 = value1 % 10;
  print(num2);
  value1 = value1 ~/ 10;
  print(value1);

  int num3 = value1 % 10;
  print(num3);
  value1 = value1 ~/ 10;
  print(value1);

  int num4 = value1 % 10;
  print(num4);
  value1 = value1 ~/ 10;
  print(value1);

  print("Sum of These numbers is ${num1 + num2 + num3 + num4}");
}
